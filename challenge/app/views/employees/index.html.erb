<div>
  <%= link_to "Get CSV Report", report_employees_path(format: :csv) %>
</div>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Start Date</th>
  </tr>
  <% @employees.each do |employee| %>
    <tr>
      <td><%= employee.name %></td>
      <td><%= employee.email %></td>
      <td><%= employee.start_date %></td>
    </tr>
  <% end %>
</table>
